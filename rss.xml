---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>johndbritton</title>
 <link href="http://johndbritton.com/feed/" rel="self"/>
 <link href="http://johndbritton.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://johndbritton.com/</id>
 <author>
   <name>John Britton</name>
   <email>public@johndbritton.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://johndbritton.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://johndbritton.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>